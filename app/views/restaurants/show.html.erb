<h2><%= @restaurant.name %></h2>
<p><%= @restaurant.address %></p>
<% @restaurant.reviews.each do |review| %>
  <ul>
    <li><p>Rating: <%= review.rating %>. Content: <%=review.content %></p></li>
  </ul>
<% end %>
<%= link_to "Add a review", new_restaurant_review_path(@restaurant) %> <br>
<%= link_to "Back to list", restaurants_path %>
